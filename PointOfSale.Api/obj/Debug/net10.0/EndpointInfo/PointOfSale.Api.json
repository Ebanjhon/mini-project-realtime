    "/api/Product/GetProductList": {
      "get": {
        "tags": [
          "Product"
        ],
        "parameters": [
          {
            "name": "textSearch",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "categoryId",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Product/CreateProduct": {
      "post": {
        "tags": [
          "Product"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "CategoryID",
                  "Price",
                  "ProductName"
                ],
                "type": "object",
                "properties": {
                  "CategoryID": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "ProductName": {
                    "maxLength": 150,
                    "minLength": 3,
                    "type": "string"
                  },
                  "Price": {
                    "type": "number",
                    "format": "double"
                  },
                  "IsActive": {
                    "type": "boolean"
                  },
                  "Description": {
                    "maxLength": 500,
                    "minLength": 0,
                    "type": "string"
                  },
                  "imageFiles": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              },
              "encoding": {
                "CategoryID": {
                  "style": "form"
                },
                "ProductName": {
                  "style": "form"
                },
                "Price": {
                  "style": "form"
                },
                "IsActive": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "imageFiles": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
